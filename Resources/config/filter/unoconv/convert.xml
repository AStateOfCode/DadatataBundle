<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="asoc_dadatata.filter.unoconv.convert.class">Asoc\Dadatata\Filter\Unoconv\Convert</parameter>
    </parameters>

    <services>
        <service id="asoc_dadatata.filter.unoconv.convert"
                 class="%asoc_dadatata.filter.unoconv.convert.class%"
                 public="false"
                 abstract="true">
            <argument>%asoc_dadatata.tools.unoconv%</argument>
            <tag name="asoc_dadatata.filter" alias="unoconv_convert" options="asoc_dadatata.filter_options.document_options"/>
        </service>
    </services>
</container>