<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="asoc_dadatata.filter_options.image_options.class">Asoc\Dadatata\Filter\ImageOptions</parameter>
        <parameter key="asoc_dadatata.filter_options.document_options.class">Asoc\Dadatata\Filter\DocumentOptions</parameter>
        <parameter key="asoc_dadatata.filter_options.pass_options.class">Asoc\Dadatata\Filter\PassOptions</parameter>
        <parameter key="asoc_dadatata.filter_options.document_image_options.class">Asoc\Dadatata\Filter\DocumentImageOptions</parameter>
    </parameters>

    <services>
        <service id="asoc_dadatata.filter_options.image_options"
                 class="%asoc_dadatata.filter_options.image_options.class%"
                 public="false"
                 abstract="true"/>
        <service id="asoc_dadatata.filter_options.document_options"
                 class="%asoc_dadatata.filter_options.document_options.class%"
                 public="false"
                 abstract="true"/>
        <service id="asoc_dadatata.filter_options.pass_options"
                 class="%asoc_dadatata.filter_options.pass_options.class%"
                 public="false"
                 abstract="true"/>
        <service id="asoc_dadatata.filter_options.document_image_options"
                 class="%asoc_dadatata.filter_options.document_image_options.class%"
                 public="false"
                 abstract="true"/>
    </services>
</container>